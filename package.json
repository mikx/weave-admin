{
  "name": "weave-admin",
  "version": "1.0.0",
  "description": "A simple administration interface for weave async server",
  "scripts": {
    "compile": "tsc --noEmit", 
    "build":   "webpack --progress --display-error-details --env.build=production",
    "build-prod": "NODE_ENV='production' webpack",
    "watch":   "npm run build -- --watch",
    "server":  "webpack-dev-server --no-inline --progress --host 0.0.0.0 --port 8000 --content-base src",
    "start":   "npm run server"
  },
  "contributors": [
    "Michael Mikhaylov <mm@izba.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.23",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "typescript": "^2.2.2",
    "zone.js": "^0.8.5",
     "reflect-metadata":"^0.1.10"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.2",
    "@ngtools/webpack": "^1.3.0",
    "@types/node": "^7.0.12",
    "css-loader": "^0.28.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "keywords": [
    "Angular2"
  ],
  "repository": {},
  "bugs": {},
  "homepage": "https://github.com/mikx/weave-admin#readme",
  "engines" : { "node" : ">=6.10.2" }
